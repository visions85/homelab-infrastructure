# Example Environment Variables
# Copy this to .env and fill in your values

# General
TZ=America/Chicago
PUID=1000
PGID=1000

# Domains
DOMAIN_NAME=example.com
AUTHENTIK_DOMAIN=auth.example.com
VAULTWARDEN_DOMAIN=vault.example.com
GRAFANA_DOMAIN=grafana.example.com

# Traefik
TRAEFIK_HTTP_PORT=80
TRAEFIK_HTTPS_PORT=443
TRAEFIK_DASHBOARD_PORT=8080

# Cloudflare (for Let's Encrypt DNS challenge)
CF_DNS_API_TOKEN=your_cloudflare_api_token_here
CF_ZONE_API_TOKEN=your_zone_token_here
ACME_EMAIL=admin@example.com

# Authentik
AUTHENTIK_SECRET_KEY=your_secret_key_here
AUTHENTIK_POSTGRES_DB=authentik
AUTHENTIK_POSTGRES_USER=authentik
AUTHENTIK_POSTGRES_PASSWORD=your_password_here

# Grafana
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your_password_here
GRAFANA_ROOT_URL=https://grafana.example.com

# InfluxDB
INFLUXDB_DB=metrics
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=your_password_here

# Vaultwarden
VAULTWARDEN_ADMIN_TOKEN=your_admin_token_here

# Paths (adjust for your system)
APPDATA_PATH=/mnt/cache/appdata
USER_PATH=/mnt/user
DOWNLOADS_PATH=/mnt/user/Downloads
